{
	"name": "csspreview",
	"displayName": "CSS Preview",
	"description": "CSS Preview is a tool for previewing selected CSS selectors in a separate column inside the vscode editor. The purpose of the tool is to give you a visual idea of your styling without leaving your editor.",
	"version": "1.0.2",
	"icon": "images/icon.png",
	"publisher": "sandorengholm",
	"author": "sandorengholm",
	"repository": {
		"type": "git",
		"url": "https://github.com/sandorengholm/csspreview"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.cssPreview"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.cssPreview",
				"title": "CSS Preview"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.cssPreview",
					"when": "resourceLangId == css"
				}
			]
		}
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
