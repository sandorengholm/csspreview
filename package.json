{
	"name": "csspreview",
	"displayName": "CSS Preview",
	"description": "CSS Preview is a tool for previewing selected CSS selectors in a separate column inside the vscode editor. The purpose of the tool is to give you a visual idea of your styling without leaving your editor.",
	"version": "1.0.1",
	"icon": "images/icon.png",
	"publisher": "sandorengholm",
	"author": "sandorengholm",
	"repository": {
		"type": "git",
		"url": "https://github.com/sandorengholm/csspreview"
	},
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.cssPreview"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.cssPreview",
				"title": "CSS Preview"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.cssPreview",
					"when": "resourceLangId == css"
				}
			]
		}
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^7.0.2",
		"@types/node": "^14.0.13",
		"typescript": "^3.9.5",
		"vscode": "^1.1.37"
	}
}
